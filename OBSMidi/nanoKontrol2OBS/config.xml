    <button midicontrolid="43" action="previoustrack()" />
    <button midicontrolid="44" action="nexttrack()" />
    <button midicontrolid="45" action="savereplay()" />
    <button midicontrolid="48" action="windowsmute(desktop1)" />
    <button midicontrolid="49" action="windowsmute(desktop2)" />
    <button midicontrolid="50" action="windowsmute(mic1)" />
    <button midicontrolid="51" action="windowsmute(mic2)" />
    <button midicontrolid="52" action="windowsmute(mic3)" />
    <button midicontrolid="64" action="obsmute(desktop1)" />
    <button midicontrolid="65" action="obsmute(desktop2)" />
    <button midicontrolid="66" action="obsmute(mic1)" />
    <button midicontrolid="67" action="obsmute(mic2)" />
    <button midicontrolid="68" action="obsmute(mic3)" />
  </inputs>
  <outputs>
    <output midicontrolid="32" event="sceneswitched" sceneindex="0"/>
    <output midicontrolid="33" event="sceneswitched" sceneindex="1"/>
    <output midicontrolid="34" event="sceneswitched" sceneindex="2"/>
