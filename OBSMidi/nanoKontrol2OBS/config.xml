<?xml version="1.0" encoding="utf-8" ?>
<config>
  <inputs>
    <slider midicontrolid="0" action="setobsvolume(desktop1)" />
    <slider midicontrolid="1" action="setobsvolume(desktop2)" />
    <slider midicontrolid="2" action="setobsvolume(mic1)" />
    <slider midicontrolid="3" action="setobsvolume(mic2)" />
    <slider midicontrolid="4" action="setobsvolume(mic3)" />
    <dial midicontrolid="16" action="setwindowsvolume(desktop1)" />
    <dial midicontrolid="17" action="setwindowsvolume(desktop2)" />
    <dial midicontrolid="18" action="setwindowsvolume(mic1)" />
    <dial midicontrolid="19" action="setwindowsvolume(mic2)" />
    <dial midicontrolid="20" action="setwindowsvolume(mic3)" />
    <button midicontrolid="32" action="switchscene(0)" />
    <button midicontrolid="33" action="switchscene(1)" />
    <button midicontrolid="34" action="switchscene(2)" />
    <button midicontrolid="35" action="switchscene(3)" />
    <button midicontrolid="36" action="switchscene(4)" />
    <button midicontrolid="37" action="switchscene(5)" />
    <button midicontrolid="38" action="switchscene(6)" />
    <button midicontrolid="39" action="switchscene(7)" />
